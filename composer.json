{
  "name": "jaymecd/travis-test",
  "description": "Travis CI test with PHP configs",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.5.9",
    "guzzlehttp/guzzle": "^5.3.0|^6.1.1",
    "monolog/monolog": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.21"
  },
  "autoload": {
    "psr-4": {
      "Trial\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trial\\Tests\\": "test"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
